          </nav>
        </li>
        <template x-for="(entry, index) in entries">
        <li class="border-(r qnx-line) flex">
          <div class="qnx-bevel">
            <div class="p-[2px] flex">
              <div class="qnx-bevel-sunk">
                <template x-if="qnxActive !== index">
                  <div class="flex absolute inset-0"><div class="qnx-bevel"></div></div>
                </template>
                <a href="#" @click.prevent="qnxActive = index" :class="['group py-1 px-3 w-full flex items-center relative motion-safe:transition md:(text-sm space-x-2)', qnxActive === index ? 'bg-qnx-dark shadow-inner' : '']">
                  <img :src="'/_assets/qnx/img/ico-'+ entry.ico +'.png'" :alt="entry.title +' icon'" class="w-[16px] h-[16px] object-contain object-center opacity-50 motion-safe:transition group-hover:(opacity-100) group-focus:(opacity-100)" />
                  <span class="hidden md:(inline-block)" x-text="entry.title"></span>
                </a>
              </div>
            </div>
          </div>
        </li>
        </template>
      </ul>
    </div>
  </div>
</footer>
{% endsetblock %}

